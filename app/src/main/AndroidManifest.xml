<?xml version="1.0" encoding="UTF-8"?>

<manifest package="com.example.pwpbcrudfirebase" xmlns:android="http://schemas.android.com/apk/res/android">


    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:theme="@style/AppTheme"
        android:supportsRtl="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:icon="@drawable/logo"
        android:allowBackup="true">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ManageData"/>
        <activity android:theme="@style/AppTheme.NoActionBar" android:name=".MainActivity"/>
    </application>
</manifest>